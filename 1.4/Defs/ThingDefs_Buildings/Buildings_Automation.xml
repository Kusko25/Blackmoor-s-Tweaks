        <defName>VFE_MilkProcessor_Oats</defName>
        <building>VFE_MilkProcessor</building>
        <items>
            <li>VCE_RawOats</li>
        </items>
        <amount>
            <li>50</li>
        </amount>
        <result>Milk</result>
        <yield>20</yield>
        <outputLimitControlled>true</outputLimitControlled>
        <maxTotalOutput>300</maxTotalOutput>
        <useQualityIncreasing>false</useQualityIncreasing>
        <singleTimeIfNotQualityIncreasing>0.1</singleTimeIfNotQualityIncreasing>
    </ItemProcessor.CombinationDef>
    <ItemProcessor.CombinationDef  MayRequire="OskarPotocki.VFE.Mechanoid">
        <defName>VFE_MilkProcessor_Soy</defName>
        <building>VFE_MilkProcessor</building>
        <items>
            <li>VCE_Soybean</li>
        </items>
        <amount>
            <li>50</li>
        </amount>
        <result>Milk</result>
        <yield>20</yield>
        <outputLimitControlled>true</outputLimitControlled>
        <maxTotalOutput>300</maxTotalOutput>
        <useQualityIncreasing>false</useQualityIncreasing>
        <singleTimeIfNotQualityIncreasing>0.1</singleTimeIfNotQualityIncreasing>
    </ItemProcessor.CombinationDef>
</Defs>