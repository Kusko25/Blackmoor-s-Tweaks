<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="BuildingBase" MayRequire="OskarPotocki.VFE.Mechanoid">
        <defName>VFE_MilkProcessor</defName>
        <label>Milk Processor</label>
        <description>Through similar processes as I imagine real productions to make milk out of vegetables (maybe they punch them?) this machine produces milk from vegetables.</description>
        <thingClass>ItemProcessor.Building_ItemProcessor</thingClass>
        <graphicData>
            <texPath>Things/Buildings/LiquorMixer/LiquorMixer</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(5,8)</drawSize>
            <shaderType>CutoutComplex</shaderType>
            <damageData>
                <cornerTL>Damage/Corner</cornerTL>
                <cornerTR>Damage/Corner</cornerTR>
                <cornerBL>Damage/Corner</cornerBL>
                <cornerBR>Damage/Corner</cornerBR>
            </damageData>
        </graphicData>
        <thingCategories>
            <li>BuildingsProduction</li>
        </thingCategories>
        <costList>
            <Steel>550</Steel>
            <ComponentIndustrial>15</ComponentIndustrial>
            <Plasteel>60</Plasteel>
            <VFE_ComponentMechanoid>1</VFE_ComponentMechanoid>
        </costList>
        <placeWorkers>
            <li>VFEMech.PlaceWorker_OnFactoryFloorAndPath</li>
        </placeWorkers>
        <building>
            <soundAmbient>GeothermalPlant_Ambience</soundAmbient>
            <wantsHopperAdjacent>true</wantsHopperAdjacent>
            <buildingTags>
                <li>Production</li>
            </buildingTags>
            <blueprintGraphicData>
                <texPath>Things/Buildings/LiquorMixer/LiquorMixer_BP</texPath>
                <graphicClass>Graphic_Multi</graphicClass>
            </blueprintGraphicData>
        </building>
        <altitudeLayer>BuildingOnTop</altitudeLayer>
        <rotatable>true</rotatable>
        <neverMultiSelect>true</neverMultiSelect>
        <uiIconScale>0.8</uiIconScale>
        <fillPercent>0.5</fillPercent>
        <useHitPoints>True</useHitPoints>
        <statBases>
            <Mass>100</Mass>
            <WorkToBuild>15000</WorkToBuild>
            <MaxHitPoints>325</MaxHitPoints>
            <Flammability>1.0</Flammability>
        </statBases>
        <tickerType>Rare</tickerType>
        <size>(3,6)</size>
        <designationCategory>VFE_Factory</designationCategory>
        <passability>PassThroughOnly</passability>
        <pathCost>70</pathCost>
        <hasInteractionCell>True</hasInteractionCell>
        <interactionCellOffset>(0,0,4)</interactionCellOffset>
        <surfaceType>Item</surfaceType>
        <comps>
            <li Class="CompProperties_Power">
                <compClass>CompPowerTrader</compClass>
                <shortCircuitInRain>false</shortCircuitInRain>
                <basePowerConsumption>700</basePowerConsumption>
            </li>
            <li Class="CompProperties_Flickable" />
            <li Class="ItemProcessor.CompProperties_ItemProcessor">
                <buildingOnGraphic>Things/Buildings/LiquorMixer/LiquorMixer</buildingOnGraphic>
                <numberOfInputs>1</numberOfInputs>
                <acceptsNoneAsInput>false</acceptsNoneAsInput>
                <transfersIngredientLists>false</transfersIngredientLists>
                <isAutoMachine>true</isAutoMachine>
                <isAutoDropper>true</isAutoDropper>
                <showFactoryProgressBar>true</showFactoryProgressBar>
                <isMachineSpecifiesOutput>true</isMachineSpecifiesOutput>
                <isCompletelyAutoMachine>true</isCompletelyAutoMachine>
                <noPowerDestroysProgress>true</noPowerDestroysProgress>
                <isMachinePausable>true</isMachinePausable>
                <rareTicksToDestroy>10</rareTicksToDestroy>
                <destroyIngredientsAtStartOfProcess>true</destroyIngredientsAtStartOfProcess>
                <chooseIngredientsIcon>UI/IP_SetOutput</chooseIngredientsIcon>
                <InsertFirstItemDesc>IP_ChooseOutput</InsertFirstItemDesc>
                <bringIngredientsText>VFEM_BringIngredients</bringIngredientsText>
                <bringIngredientsDesc>VFEM_BringIngredientsDesc</bringIngredientsDesc>
                <bringIngredientsIcon>UI/IP_Begin</bringIngredientsIcon>
                <inputSlots>
                    <li>(1,0,-3)</li>
                </inputSlots>
            </li>
        </comps>
        <minifiedDef>MinifiedThing</minifiedDef>
        <researchPrerequisites Inherit='False'>
            <li>VFE_ProvisionsPackagingFactory</li>
        </researchPrerequisites>
        <modExtensions>
            <li Class="VanillaFurnitureExpanded.ShowBlueprintExtension">
                <showBlueprintInGhostMode>true</showBlueprintInGhostMode>
            </li>
        </modExtensions>
        <constructionSkillPrerequisite>8</constructionSkillPrerequisite>
    </ThingDef>
    <!--Combinations-->
    <ItemProcessor.CombinationDef  MayRequire="OskarPotocki.VFE.Mechanoid">
        <defName>VFE_MilkProcessor_Rice</defName>
        <building>VFE_MilkProcessor</building>
        <items>
            <li>RawRice</li>
        </items>
        <amount>
            <li>50</li>
        </amount>
        <result>Milk</result>
        <yield>20</yield>
        <outputLimitControlled>true</outputLimitControlled>
        <maxTotalOutput>300</maxTotalOutput>
        <useQualityIncreasing>false</useQualityIncreasing>
        <singleTimeIfNotQualityIncreasing>0.1</singleTimeIfNotQualityIncreasing>
    </ItemProcessor.CombinationDef>
